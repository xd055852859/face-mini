/**app.wxss**/

.container {
    min-height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    /* justify-content: space-between; */
    box-sizing: border-box;
}

page {
    height: 100%;
}

.button-group {
    position: absolute;
    width: 432rpx;
    height: 90rpx;
    line-height: 90rpx;
    left: calc(50% - 216rpx);
}

.button-image {
    position: absolute;
    width: 432rpx;
    height: 90rpx;
    line-height: 90rpx;
    left: calc(50% - 216rpx);
}

.online-text {
    text-decoration: underline;
    color: #85a8e6;
}
